<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="kpiWeightNewForZdmNto" class="ru.ipolynkina.beans.kpiweight.KPIWeightNewForZdmNto">
        <constructor-arg value="${kpiWeight}"/>
        <constructor-arg value="${zdmNtoPost}"/>
        <constructor-arg value="${useWithoutCategory}"/>
        <constructor-arg value="${minCategory}"/>
        <constructor-arg value="${maxCategory}"/>
        <constructor-arg value="${amount}"/>
        <constructor-arg value="${type}"/>
        <constructor-arg value="${nomenclature}"/>
        <constructor-arg value="${zdmNtoNewValue}"/>
        <constructor-arg value="${activate}"/>
    </bean>

    <bean id="kpiWeightOldForZdmNto" class="ru.ipolynkina.beans.kpiweight.KPIWeightOldForZdmNto">
        <constructor-arg value="${kpiWeight}"/>
        <constructor-arg value="${zdmNtoPost}"/>
        <constructor-arg value="${useWithoutCategory}"/>
        <constructor-arg value="${minCategory}"/>
        <constructor-arg value="${maxCategory}"/>
        <constructor-arg value="${amount}"/>
        <constructor-arg value="${type}"/>
        <constructor-arg value="${nomenclature}"/>
        <constructor-arg value="${zdmNtoOldValue}"/>
        <constructor-arg value="${activate}"/>
    </bean>

</beans>
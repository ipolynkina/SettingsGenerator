<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="kpiWeightNewForZdmTo" class="ru.ipolynkina.beans.kpiweight.KPIWeightNewForZdmTo">
        <constructor-arg value="${kpiWeight}"/>
        <constructor-arg value="${zdmToPost}"/>
        <constructor-arg value="${useWithoutCategory}"/>
        <constructor-arg value="${minCategory}"/>
        <constructor-arg value="${maxCategory}"/>
        <constructor-arg value="${amount}"/>
        <constructor-arg value="${type}"/>
        <constructor-arg value="${nomenclature}"/>
        <constructor-arg value="${zdmToNewValue}"/>
        <constructor-arg value="${activate}"/>
    </bean>

    <bean id="kpiWeightOldForZdmTo" class="ru.ipolynkina.beans.kpiweight.KPIWeightOldForZdmTo">
        <constructor-arg value="${kpiWeight}"/>
        <constructor-arg value="${zdmToPost}"/>
        <constructor-arg value="${useWithoutCategory}"/>
        <constructor-arg value="${minCategory}"/>
        <constructor-arg value="${maxCategory}"/>
        <constructor-arg value="${amount}"/>
        <constructor-arg value="${type}"/>
        <constructor-arg value="${nomenclature}"/>
        <constructor-arg value="${zdmToOldValue}"/>
        <constructor-arg value="${activate}"/>
    </bean>

</beans>